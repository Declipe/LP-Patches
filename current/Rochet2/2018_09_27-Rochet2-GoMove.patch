+    if(msg ~= '') then
+        for k, v in pairs(GOMove.SCMD) do
+            if(type(k) == "string" and string.find(k, msg:lower()) == 1 and type(v) == "function") then
+                v()
+                break;
+            end
+        end
+        return
+    end
+    if(MainFrame:IsVisible()) then
+        MainFrame:Hide()
+    else
+        MainFrame:Show()
+    end
+end
