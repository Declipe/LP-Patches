+function GOMove.SCMD.invertselection()
+    local sel = {}
+    for GUID, NAME in pairs(GOMove.Selected) do
+        if(tonumber(GUID)) then
+            table.insert(sel, GUID)
+        end
+    end
+    for k, tbl in ipairs(SelFrame.DataTable) do
